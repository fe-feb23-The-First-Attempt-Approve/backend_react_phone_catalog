{
  "name": "product-catalog-back-end",
  "version": "0.0.0",
  "private": true,
  "main": "src/server.ts",
  "scripts": {
    "start": "npx ts-node src/server.ts",
    "sync": "npx ts-node src/sync.ts",
    "build": "npm i && tsc -p .",
    "prettier": "npx prettier --write . --ignore-path .gitignore",
    "lint": "eslint . --ignore-path .gitignore",
    "fix-style": "npm run prettier && npm run lint -- --fix",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.js": "npm run lint",
    "*.ts": "npm run lint"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.1.4",
    "express": "^4.18.2",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1",
    "pg": "^8.11.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.32.0",
    "sequelize-typescript": "^2.1.5"
  },
  "devDependencies": {
    "@mate-academy/eslint-config": "0.0.25",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^20.2.5",
    "@types/validator": "^13.7.17",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "eslint": "^8.42.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.2",
    "prettier": "2.8.8",
    "typescript": "^5.1.3",
    "ts-node": "^10.9.1"
  }
}
